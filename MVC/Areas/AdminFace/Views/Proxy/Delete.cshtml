@model ProxyGateway.Middleware.ProxyDataModel
@using MVC
@using MVC.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, MVC
@addTagHelper *, ViewsCommon
@addTagHelper *, TableView
@addTagHelper *, FormView
@addTagHelper *, TreeView
@addTagHelper *, GridView
@addTagHelper *, ChartView
@addTagHelper *, ComboboxView
@addTagHelper *, ContextMenuView
@{
    ViewData["Title"] = "Удаление сегмента приложения";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h3>Подтвердите удаление?</h3>
<div>
    <h4>Сегмент приложения</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Path)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Path)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Key)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Key)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.URL)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.URL)
        </dd>
    </dl>

    <form action="Delete">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a href="/AdminFace/Proxy/Index">Назад</a>
    </form>
</div>
